{
    "name": "chatons",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "lint": "ng lint",
        "doc": "compodoc -p ./src/tsconfig.json",
        "prestart": "npm install && npm run build && npm run doc",
        "build:img": "docker build --tag chaton-front ./",
        "docker:run": "docker run -d -p 80:80 --name chatons chaton-front",
        "docker:clean": "docker stop chatons && docker rm chatons",
        "deploy": "npm run docker:clean && npm run build:img && npm run docker:run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.2.2",
        "@angular/cdk": "~7.3.6",
        "@angular/common": "^7.2.2",
        "@angular/compiler": "^7.2.2",
        "@angular/core": "^7.2.2",
        "@angular/forms": "^7.2.2",
        "@angular/http": "^7.2.2",
        "@angular/material": "^7.3.6",
        "@angular/platform-browser": "^7.2.2",
        "@angular/platform-browser-dynamic": "^7.2.2",
        "@angular/router": "^7.2.2",
        "bootstrap": "^4.3.1",
        "core-js": "^2.6.3",
        "hammerjs": "^2.0.8",
        "rxjs": "^6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.0",
        "@angular/cli": "^7.2.3",
        "@angular/compiler-cli": "^7.2.2",
        "@angular/language-service": "^7.2.2",
        "@compodoc/compodoc": "^1.1.7",
        "@types/node": "~8.9.4",
        "codelyzer": "^4.5.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "^3.2.4"
    }
}
